{
	"scopeName": "source.oml",
	"name": "Oml",
	"fileTypes": ["oml"],
	"patterns": [
		{
			"name": "keyword.other.oml",
			"match": "(?<=[^\\b^]|^)(all|annotation|as|aspect|asymmetric|bundle|ci|concept|description|differentFrom|domain|entity|enumerated|exactly|extends|forward|from|functional|includes|inverse|irreflexive|key|language|length|max|maxExclusive|maxInclusive|maxLength|min|minExclusive|minInclusive|minLength|pattern|property|range|ref|reflexive|relation|restricts|reverse|ri|rule|sameAs|scalar|some|structure|structured|symmetric|to|transitive|uses|vocabulary|with)\\b"
		},
		{
			"name": "markup.inline.raw.oml",
			"begin": "<",
			"end": ">"
		},
		{
			"name": "string.quoted.double.oml",
			"begin": "\"",
			"end": "\""
		},
		{
			"name": "string.quoted.single.oml",
			"begin": "'",
			"end": "'"
		},
		{
			"name": "comment.line.oml",
			"match": "//.+$"
		},
		{
			"name": "comment.block.oml",
			"begin": "/\\*",
			"end": "\\*/"
		}
	]
}